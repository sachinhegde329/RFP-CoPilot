rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /tenants/{tenantId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /tenants/{tenantId}/{subcollection}/{docId=**} {
      allow read, write: if request.auth != null;
    }
  }
}